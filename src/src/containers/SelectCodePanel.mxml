<?xml version="1.0" encoding="utf-8"?>
<mx:TitleWindow xmlns:mx="http://www.adobe.com/2006/mxml" layout="absolute" width="100" 
			   height="150"  close="close()" showCloseButton ="true">

	<mx:Script>

		<![CDATA[
			
			private function close():void
			{
				if(this.parent)this.parent.removeChild(this);
			}
			
					
		]]>
		
	</mx:Script>
	<mx:Label x="10" y="10" text="类名" id="Label"/>
	<mx:TextArea x="50" y="10" width="40" height="20" id="cName"/>
	<mx:CheckBox x="10" y="50" label="是否单例"  id="danli"/>
	<mx:Button x="10" y="80" label="确定" width="72" height="36" id="codeButton" />
</mx:TitleWindow>
